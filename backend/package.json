  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "db:up": "docker compose up -d db",
    "db:test:up": "docker compose up -d db_test",
    "db:down": "docker compose down",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:test:push": "prisma db push --skip-generate --schema prisma/schema.prisma",
    "prisma:test:reset": "prisma migrate reset --force --skip-seed",
    "pretest": "npm run db:test:up && node ./scripts/wait-for-db.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "quantum:security:scan": "npm run quantum-security-scanner",
    "quantum:health:check": "npm run quantum-health-checker",
    "quantum:performance:test": "npm run quantum-performance-tester",
    "quantum:compliance:check": "npm run quantum-compliance-checker",
    "quantum:deploy": "npm run quantum-deployment-script",
    "quantum:monitor": "npm run quantum-monitoring-script"
